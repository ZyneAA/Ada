{
    "theme_name": "Sunset Serenade",
    "name": "serenade",
    "title": "Embrace the Sunset Serenade",
    "description": "A calming theme inspired by the warm and soothing hues of a sunset, perfect for relaxed coding sessions.",
    "bg_class": "bg-serenade",
    "editor": {
        "foreground": "#F5F0E1",
        "background": "#3C2A4B",
        "background_complement": "#6A4F6E",
        "background_second_complement": "#F7F3F4",
        "font": "#F8F5F2",
        "cursor_foregorund": "#E0C6A8",
        "line_highlight_background": "#6A4F6E",
        "line_number_foreground": "#D8A6C0",
        "selection_background": "#A87D9B",
        "inactive_selection_background": "#3C2A4B",
        "dintinct_color": "#F7F3F4",
        "rules": [
            {
                "token": "identifier",
                "foreground": "#E4B7A0",
                "fontStyle": "normal"
            },
            {
                "token": "keyword",
                "foreground": "#F6D8E2",
                "fontStyle": "bold"
            },
            {
                "token": "string",
                "foreground": "#F6D8E2",
                "fontStyle": "normal"
            }    
        ]
    }
}